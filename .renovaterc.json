{
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["^.env$"],
      "matchStrings": ["GITLAB_TAG=(?<currentValue>.*?)\n"],
      "depNameTemplate": "gitlab/gitlab-ce",
      "datasourceTemplate": "docker",
      "versioningTemplate": "loose"
    }
  ],
  "packageRules": [
    {
      "packagePatterns": ["^gitlab\/gitlab-.+$"],
      "automerge": true
    }
  ]
}
